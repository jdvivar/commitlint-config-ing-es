{
  "name": "commitlint-config-ing-es",
  "version": "1.0.1",
  "description": "Shareable commitlint config enforcing ING Spain issue format",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdvivar/commitlint-config-ing-es"
  },
  "keywords": [
    "commitlint"
  ],
  "scripts": {
    "release": "standard-version"
  },
  "author": "Daniel Vivar <mail@danielvivar.com>",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "husky": "^4.2.5",
    "standard": "^14.3.4",
    "standard-version": "^8.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
